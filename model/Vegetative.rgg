double ts0 (int rank)
{
	float tmp = 0;
	if (rank<=10) {
		tmp = 100;
	} else if(rank>10 && rank<=14 ) {
		tmp = 100 + 10*rank;
	}
	return tmp + ConstPara.PLASTOCHRON * rank;
}

