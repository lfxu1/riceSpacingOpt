private int getStage()  //lfxu110104, from 1-9  !notice: the index should be stage-1
{
	int t = RiceBreeder.INSTANCE.breedTime;
	int stage = 0;
	if(t<30){
		stage = 1;
	} else if(t>=30 && t<120) {
		stage = (t-30)/10 + 1;
	} else if(t>=120) {
		stage = 9;
	}
	//println("-------current stage: " + stage);
	return stage;
}


