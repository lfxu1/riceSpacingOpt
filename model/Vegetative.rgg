module Internode (super.length, super.radius, int rank, int order, int tb, int age)
		extends Cylinder(length, radius/2.0).(setShader(shmat))
{
	int appearDay = 0;
	float agr = 0; //actual growth rate
	float agr_radius = 0;
	float agr2 = 0; //ignore limit(<=pgr),wy131121add
